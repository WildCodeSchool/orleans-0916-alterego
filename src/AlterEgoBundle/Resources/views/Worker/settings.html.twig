{% extends 'AlterEgoBundle:Worker:layout_worker.html.twig' %}

{% block head %}
    {{ parent() }}
{% endblock %}

{% block content %}
    <div class="container-fluid">
        <div class="row">
            <div class="col-xs-11 top_worker">
                <img class="img_top_w" src="{{ asset ('assets/images/logo-safran.png') }}" alt="">
            </div>
        </div>


        <div class="row">
            <div class="top_worker_by">
                <h2 class="col-xs-6 by">by</h2>
                <img class="col-xs-6 img_top_right_w" src="{{ asset ('assets/images/logo_notext.png') }}" alt="">
            </div>
        </div>
    </div>
    <div class="container-fluid">
        <div class="row">
            <div class="flipswitch">
                <input type="checkbox" name="flipswitch" class="flipswitch-cb" id="fs" checked>
                <label class="flipswitch-label" for="fs">
                    <div class="flipswitch-inner"></div>
                    <div class="flipswitch-switch"></div>
                </label>
            </div>

        </div>
    </div>
{% endblock %}